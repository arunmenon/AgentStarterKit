    "        ax4.bar(users, interactions, color='#45B7D1', alpha=0.7)\n",
    "        ax4.set_title('Interactions by User')\n",
    "        ax4.set_xlabel('User ID')\n",
    "        ax4.set_ylabel('Number of Interactions')\n",
