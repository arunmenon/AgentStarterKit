    "        print(f\"  • Completed: {execution_result['tasks_completed']}\")\n",
    "        print(f\"  • Failed: {execution_result['tasks_failed']}\")\n",
    "        print(f\"  • Success rate: {(execution_result['tasks_completed'] / len(plan.tasks) * 100):.1f}%\")\n",
    "        print(f\"  • Duration: {execution_result.get('total_duration', 0):.1f} seconds\")\n",
    "        print(f\"  • Replanning events: {execution_result['replanning_events']}\")\n",
    "        print(f\"  • Error recoveries: {execution_result['error_recoveries']}\")\n",
    "\n",
    "# Create the adaptive executor\n",
