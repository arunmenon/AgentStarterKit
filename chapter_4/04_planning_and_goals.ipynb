   "source": [
    "## Plan Executor\n",
    "\n",
    "Executes plans with monitoring and adaptation."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "class PlanExecutor:\n",
    "    def __init__(self):\n",
    "        self.execution_history = []\n",
    "        self.current_plan = None\n",
    "\n",
    "    def execute_plan(self, plan: Plan) -> Dict[str, Any]:\n",
    "        self.current_plan = plan\n",
    "        print(f\"\\nExecuting plan: {plan.goal}\")\n",
    "        \n",
    "        execution_stats = {\n",
    "            'tasks_completed': 0,\n",
    "            'tasks_failed': 0,\n",
    "            'total_tasks': len(plan.tasks),\n",
    "            'start_time': datetime.now()\n",
    "        }\n",
    "\n",
    "        while not plan.is_complete():\n",
    "            ready_tasks = plan.get_ready_tasks()\n",
    "            \n",
    "            if not ready_tasks:\n",
    "                print(\"No ready tasks - checking for failures\")\n",
    "                break\n",
    "\n",
    "            # Execute highest priority ready task\n",
    "            task = ready_tasks[0]\n",
    "            print(f\"Executing task: {task.name}\")\n",
    "            \n",
    "            # Simulate task execution\n",
    "            import random\n",
    "            import time\n",
    "            time.sleep(0.1)  # Simulate work\n",
    "            \n",
    "            success = random.random() > 0.2  # 80% success rate\n",
    "            \n",
    "            if success:\n",
    "                task.status = TaskStatus.COMPLETED\n",
    "                execution_stats['tasks_completed'] += 1\n",
    "                print(f\"  -> Completed: {task.name}\")\n",
    "            else:\n",
    "                task.status = TaskStatus.FAILED\n",
    "                execution_stats['tasks_failed'] += 1\n",
    "                print(f\"  -> Failed: {task.name}\")\n",
    "\n",
    "        execution_stats['end_time'] = datetime.now()\n",
    "        execution_stats['duration'] = (\n",
    "            execution_stats['end_time'] - execution_stats['start_time']\n",
    "        ).total_seconds()\n",
    "\n",
    "        self.execution_history.append(execution_stats)\n",
    "        return execution_stats\n",
    "\n",
    "    def get_performance_summary(self):\n",
    "        if not self.execution_history:\n",
    "            return \"No executions completed yet\"\n",
    "        \n",
    "        total_tasks = sum(h['total_tasks'] for h in self.execution_history)\n",
    "        completed_tasks = sum(h['tasks_completed'] for h in self.execution_history)\n",
    "        success_rate = completed_tasks / total_tasks * 100 if total_tasks > 0 else 0\n",
    "        \n",
    "        return f\"Overall success rate: {success_rate:.1f}% ({completed_tasks}/{total_tasks})\"\n",
    "\n",
    "# Create executor\n",
    "executor = PlanExecutor()\n",
    "print(\"Plan executor created\")"
   ]
  },
