    "## Custom Decomposition Rules\n",
    "\n",
    "Define how specific types of goals should be decomposed."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def analysis_decomposition(goal: str) -> List[Task]:\n",
    "    \"\"\"Decompose analysis-type goals\"\"\"\n",
    "    base_id = str(uuid.uuid4())[:4]\n",
    "    \n",
    "    gather_task = Task(\n",
    "        id=f\"{base_id}_1\",\n",
    "        name=\"Gather Data\",\n",
    "        description=\"Collect and organize relevant data\",\n",
    "        estimated_duration=45,\n",
    "        priority=5\n",
    "    )\n",
    "    \n",
    "    analyze_task = Task(\n",
    "        id=f\"{base_id}_2\",\n",
    "        name=\"Analyze Data\",\n",
    "        description=\"Perform statistical and qualitative analysis\",\n",
    "        dependencies=[gather_task.id],\n",
    "        estimated_duration=90,\n",
    "        priority=4\n",
    "    )\n",
    "    \n",
    "    report_task = Task(\n",
    "        id=f\"{base_id}_3\",\n",
    "        name=\"Generate Report\",\n",
    "        description=\"Create comprehensive analysis report\",\n",
    "        dependencies=[analyze_task.id],\n",
    "        estimated_duration=60,\n",
    "        priority=3\n",
    "    )\n",
    "    \n",
    "    return [gather_task, analyze_task, report_task]\n",
    "\n",
    "def research_decomposition(goal: str) -> List[Task]:\n",
    "    \"\"\"Decompose research-type goals\"\"\"\n",
    "    base_id = str(uuid.uuid4())[:4]\n",
    "    \n",
    "    return [\n",
    "        Task(\n",
    "            id=f\"{base_id}_1\",\n",
    "            name=\"Literature Review\",\n",
    "            description=\"Review existing research and sources\",\n",
    "            estimated_duration=120,\n",
    "            priority=5\n",
    "        ),\n",
    "        Task(\n",
    "            id=f\"{base_id}_2\",\n",
    "            name=\"Primary Research\",\n",
    "            description=\"Conduct original research activities\",\n",
    "            dependencies=[f\"{base_id}_1\"],\n",
    "            estimated_duration=180,\n",
    "            priority=4\n",
    "        ),\n",
    "        Task(\n",
    "            id=f\"{base_id}_3\",\n",
    "            name=\"Synthesize Findings\",\n",
    "            description=\"Combine and interpret research results\",\n",
    "            dependencies=[f\"{base_id}_2\"],\n",
    "            estimated_duration=90,\n",
    "            priority=3\n",
    "        )\n",
    "    ]\n",
    "\n",
    "# Register decomposition rules\n",
    "planner.register_decomposition(\"analysis\", analysis_decomposition)\n",
    "planner.register_decomposition(\"research\", research_decomposition)\n",
    "\n",
    "print(\"Custom decomposition rules registered\")"
   ]
